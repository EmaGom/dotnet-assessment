{
  "AppBasePath": "assessment/api",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "RequestPath like '%/health'"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "SqlConnection": "Server=tcp:timechimpassessmentserver.database.windows.net,1433;Initial Catalog=timechimpassessmentdb;Persist Security Info=False;User ID=dbadmin;Password=timechimpassessment#1;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
  },
  "CacheConfiguration": {
    "AbsoluteExpirationInMinutes": 2,
    "SlidingExpirationInMinutes": 2
  },
  "Auth0": {
    "Authority": "https://timechimp.eu.auth0.com/",
    "Identifier": "api:gateway"
  },
  "Swagger": {
    "ServiceOptions": {
      "ApiTitle": "timechimp.backend.assessment",
      "ApiDescription": "timechimp.backend.assessment"
    },
    "Auth0Options": {
      "ApplicationName": "API Swagger UI",
      "Audience": "api:gateway"
    }
  },
  "ElasticConfiguration": {
    "Uri": "http://10.140.178.92:9200/"
  },
  "DistributedCacheOptions": {
    "UseMemoryCache": true,
    "InstanceName": "backend-assessment"
  }
}